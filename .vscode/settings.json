{
  "prettier.configPath": ".prettierrc",
  "eslint.enable": true,
  "cSpell.words": ["nuxtjs", "Parens", "tanstack"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": false
  },
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
